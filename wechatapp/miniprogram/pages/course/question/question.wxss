page {
  height: 100%;
}
.wrapper {
  /* position: fixed; */
  height: 100%;
  width: 100%;
  overflow-y: auto;
  background-color: #EBF0F5;
}

.wrapper .header{
  position: fixed;
  z-index: 999999;
  top: 0;
  left: 0;
  width: 100%;
  height: 42rpx;
  background-color: #fff;
  display: flex;
  /* justify-content: space-between; */
  align-items: center;
}
.wrapper .header .left{
  padding-left: 53rpx;
  padding-right: 23rpx;
  box-sizing: border-box;
  color: #fff;
  display: flex;
  justify-content: center;
  height: 100%;
  background-color: #FFB71F;
  border-radius: 0 42rpx 42rpx 0;
  flex-direction: column;
  font-size: 16rpx;
}
.wrapper .header .left .index,.wrapper .header .left .type  {
  font-size: 12rpx;
  font-weight: bold;
  display: flex;
  align-content: center;
}
.wrapper .header .middle{
  /* font-size: 8rpx; */
  padding-right: 50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.wrapper .header .middle .item{
  display: flex;
  justify-content: center;
  flex-direction: row;
  align-items: center;
  padding: 0 11rpx;
}
.wrapper .header .middle .score, .answer-footer .right .item .score{
  font-size: 15rpx;
  color: #5CB3FF;
  font-weight: bold;
  padding-left: 4rpx;
}
.wrapper .header .middle .title, .answer-footer .right .item .title{
  font-size: 12rpx;
  font-weight: bold;
}
.wrapper .header .right{
  padding-right: 50rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  text-align: right;
  flex: 1
}

#backBtn{
  width: 32rpx;
  height: 32rpx;
  position: absolute;
  top: 10rpx;
  left: 16rpx;
  z-index:99999999;
}
.container {
  min-height: 100vh;
  width: 100%;
  padding: 50rpx 51rpx 66rpx 51rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.container .image-wrapper{
  width: 100%;
  display: flex;
  align-items: flex-start;
  flex-direction: column;
  margin-bottom: 10rpx;
}
.container .answer-pic{
  width: 100%;
}
.container .answer-pic-1{
  margin-top: 10rpx;
}

.chat-bottom {
  display: flex;
  justify-content: space-between;
  background: #fff;
  padding: 24rpx 16rpx 13rpx;
  box-sizing: border-box;
  width:100%;
  min-height:320rpx;
  /* margin-top: 10rpx; */
}
.chat-bottom .center {
  flex: 2;
  margin: 0 18rpx 0rpx 0rpx;
  font-size: 16rpx;
}
.chat-bottom .center .table{
  border-radius: 2rpx;
  border:1px solid rgba(196,200,204,1);
}
.chat-bottom .right {
  flex: 0 0 125rpx;
  padding: 13rpx 48rpx 0 14rpx;
  
  background:rgba(255,96,71,.08);
  border-left: 4rpx solid #FF6047;
}
.chat-bottom .right .title{
  font-size: 16rpx;
}
.chat-bottom .right .student-wrapper-1{
  margin-top: 16rpx;
  font-size: 16rpx;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
/* .chat-bottom .right .student-wrapper .item{
  display: flex;
  justify-content: space-between;
  align-items: center;
} */
.chat-bottom .center .table .thead{
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid rgba(196,200,204,1);
}
.chat-bottom .center .table .tbody{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.chat-bottom .center .table .tbody .td {
  flex: 1;
  text-align: left;
  height: 46rpx;
  line-height: 46rpx;
  color:#FF6047;
}
.chat-bottom .center .table .active {
  background-color: #C4C8CC;
}
.chat-bottom .center .table .isRight .td{
  color: #5CB3FF;
}
.chat-bottom .center .table .tbody .td:first-child,.chat-bottom .center .table .thead .td:first-child {
  padding-left: 32rpx;
}
.chat-bottom .center .table .thead .td{
  flex: 1;
  text-align: left;
  font-size: 16rpx;
  color: #1D2023;
  height: 46rpx;
  line-height: 46rpx;
}
.container-column {
  height: 148rpx;
  width: 148rpx;
}

.answer-knowlage{
  width: 100%;
  background-color: #818D99;
  padding:6rpx 8rpx 5rpx 21rpx;
  color: #fff;
  display:flex;
  box-sizing: border-box;
  flex-direction: row;
  font-size: 12rpx;
  justify-content: space-between;
}
.answer-knowlage .left{
  vertical-align: middle;
  padding-top: 5rpx;
}
.answer-knowlage-2{
  position: fixed;
  bottom: 46rpx;
  left: 0;
  z-index: 99999;
}

.answer-knowlage .left text:first{
  font-weight: bold;
}

.answer-knowlage .right .practice{
  border: 1px solid #fff;
  color: #fff;
  border-radius: 10rpx;
  padding: 6rpx 10rpx;
  font-size: 12rpx;
  justify-self: flex-end;
}


.answer-footer {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  background: #fff;
  padding: 0 21rpx;
  box-sizing: border-box;
  height: 46rpx;
  font-size: 14rpx;
  display: flex;
  z-index: 99999999999;
  justify-content: space-between;
}
.answer-footer .left,.answer-footer .right{
  flex: 1;
  display: flex;
  align-items: center;
}
.answer-footer .left {
  justify-content: flex-start;
}
.answer-footer .left .title{
  font-size: 11rpx;
  margin-right: 16rpx;
}
.answer-footer .right {
  justify-self: flex-end;
}
.answer-footer .left .number-wrapper{
  display: flex;
  justify-content: space-between;
  border:1px solid rgba(92,179,255,1);
  border-radius:1px;
}
 .answer-footer .left .number-wrapper .active{
   background:rgba(92,179,255,.24);
 }
.answer-footer .left .number-wrapper .item{
  font-size: 11rpx;
  width: 55rpx;
  height: 33rpx;
  align-items: center;
  flex-direction: column;
  display: flex;
  justify-content: center;
  box-sizing: border-box;
}
.answer-footer .left .number-wrapper .item:not(:last-child){
  border-right:1px solid rgba(92,179,255,1);
}

.answer-footer .right .item{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
}
.answer-footer .right .item:nth-child(2){
  margin: 0 33rpx;
}
/* .answer-footer .right .item view:nth-child(1){
  color: #5CB3FF;
  font-weight: bold;
}
.answer-footer .right .item view:nth-child(2){
  color: #1D2023;
} */

.layer {
  position: fixed;
  height: 100%;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 99999999;
  background:rgba(25,25,26,.32);
}
.student-wrapper {
  position: fixed;
  height: calc(100% - 58rpx);
  width: calc(100% - 42rpx);
  top: 8rpx;
  left: 21rpx;
  z-index: 999999999;
  display: flex;
  justify-content: space-between;
}


.student-wrapper .left{
  flex: 0 0 90rpx;
  background-color: #fff;
}
.student-wrapper .left .search{
  background-color:  #EBF0F5;
  margin: 8rpx;
}
.student-wrapper .left .search .search-icon{
 height: 6rpx;
 width: 6rpx;
 margin: 6rpx 6rpx 7rpx 7rpx;
}
.student-wrapper .left .title {
  border-bottom:1px solid #E1E3E6;
}
.student-wrapper .left .title ,.student-wrapper .left .item,.student-wrapper .left .search{
  display: flex;
  height: 40rpx;
  justify-content: space-between;
  align-items: center;
  padding: 0 13rpx;
  color: #818D99;
  font-size: 12rpx;
  box-sizing: border-box;
}
.student-wrapper .left .item {
  color: #1D2023;
}
.student-wrapper .left .active {
  background-color: #5CB3FF;
  color: #fff;
}
.student-wrapper  .right{
  flex: 1;
  background-color: #EBF0F5;
  display: flex;
  align-items: center;
  justify-content: center;
}
.student-wrapper  .right image{
  width: calc(100% - 16rpx)
}