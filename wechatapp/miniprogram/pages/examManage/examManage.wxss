.tab .btn{
    display: inline-block;
    color: #1D2023;
    font-size: 32rpx;
    width: 198rpx;
    height: 72rpx;
    line-height: 72rpx;
    text-align: center;

    border:1px solid rgba(196,200,204,1);
    border-radius:36rpx;
}
.tab .btn1{
  border-right:none; 

  border-top-right-radius: 0 ;
  border-bottom-right-radius: 0 ;
  padding-right: 60rpx;

   position: relative;
   right: -70rpx;
}
.tab .btn2{
  border-left:none; 
  border-top-left-radius: 0 ;
  border-bottom-left-radius: 0 ;
  padding-left: 60rpx;
}
.tab .active1{
   color: #FFFFFF;
   background: #565E66;
   /* border: 1px solid #FF7A00; */
   position: relative;
   right: -70rpx;

   border-top-right-radius: 36rpx ;
  border-bottom-right-radius: 36rpx ;
  padding-right: 0rpx;
 }
 .tab .active2{
   color: #FFFFFF;
   background: #565E66;
   /* border: 1px solid #FF7A00; */
   padding-left: 0rpx;
   border-top-left-radius: 36rpx ;
   border-bottom-left-radius:  36rpx ;
   position: relative;
   left: 0rpx;
 }
 .tab .end{
   position: absolute;
   right: 36rpx;
   top: 38rpx;
   /* width: 40rpx;
   height: 36rpx;  */
   display: flex; 
   flex-direction: column;
   align-items: center;
 }
.tab .end .c{
   font-size: 32rpx;
   color: #CBCBCC;
   margin-left: 10rpx;
}
.tab .end .img_end{
   width: 40rpx;
   height: 36rpx;  
}
