.container {
  position: fixed;
  height: 100%;
  width: 100%;
  top: 0;
  padding: 88rpx 32rpx 176rpx;
  box-sizing: border-box;
  background-color: rgba(235,240,245,1);
  left: 0;
  overflow-y: scroll;
}

.header-wrapper {
  padding: 0rpx 24rpx 0rpx 24rpx;
  background-color: #fff;
  border-radius:16rpx;
}
.header:nth-child(1) {
  border-top-left-radius: 6rpx;
  border-top-right-radius: 6rpx;
}
.header:nth-child(2) {
  border-bottom-left-radius: 6rpx;
  border-bottom-right-radius: 6rpx;
}
.header-warn {
  padding-bottom: 24rpx;
  border-bottom: 1rpx solid rgba(235,240,245,1);
}
.header-warn .top  {
  display: flex;
  justify-content: flex-start;
  /* align-items: center; */
  margin-bottom: 16rpx;
  margin-top: 20rpx;
  height: 40rpx;
  line-height: 54rpx;
  vertical-align: bottom;
}
.header-warn .top text{
  margin-right: 47rpx;
  font-size: 28rpx;
  color: #818D99;
}
.header-warn .top input{
  flex: 1;
  color: #19191A;
  font-weight: bold;
  font-size: 28rpx;
  box-sizing: border-box;
}
.header-warn .warn {
  height: 40rpx;
  width: 100%;
  line-height: 40rpx;
  text-align: center;
  color: #5CB3FF;
  font-size: 24rpx;
  background:rgba(92,179,255,.16);
  border-radius:4rpx;
}
.header {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.header image{
  width: 24rpx;
  height: 10rpx;
  vertical-align:middle;
}
.header text{
  margin-right: 47rpx;
  font-size: 28rpx;
  color: #818D99;
  line-height: 88rpx;
}
.header input{
  flex: 1;
  color: #19191A;
  font-weight: bold;
  font-size: 28rpx;
  line-height: 88rpx;
  box-sizing: border-box;
}
.border-bottom {
  border-bottom: 1rpx solid rgba(235,240,245,1);
  box-sizing: border-box;
}
.header-table {
  width: 100%;
  border-radius:6rpx;
}
.header-table .thead{
  display: flex;
  justify-content: space-between;
  width: 100%;
  margin-top: 24rpx;
  border-top-left-radius: 6rpx;
  border-top-right-radius: 6rpx;
}
.header-table .thead{
  background-color: #fff;
}
.header-table .thead view{
  flex: 1;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  color: #A1A2A6;
  font-size: 28rpx;
}
.header-table .thead view:nth-child(1),
.header-table .thead view:nth-child(2),
.header-table .thead view:nth-child(3),
.header-table .thead view:nth-child(4),
.header-table .thead view:nth-child(5),
.header-table .thead view:nth-child(6){
  border-right: 1rpx solid #E1E3E6;
  box-sizing: border-box;
}

.header-table .tbody{
  display: flex;
  justify-content: space-between;
  width: 100%;
  background-color: #fff;
}
.header-table .tbody view{
  flex: 1;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  color: #A1A2A6;
  border-top: 1rpx solid #E1E3E6;
  box-sizing: border-box;
  font-size: 28rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.header-table .tbody view input{
  border: none;
  color: #19191A;
  flex:  0 0 40rpx;
  background: rgba(0,0,0,0);
}
.header-table .tbody view:nth-child(1),
.header-table .tbody view:nth-child(2),
.header-table .tbody view:nth-child(3),
.header-table .tbody view:nth-child(4),
.header-table .tbody view:nth-child(5),
.header-table .tbody view:nth-child(6){
  border-right: 1rpx solid #E1E3E6; 
  border-left: none; 
}

/* 悬浮按钮分数 */
.score-wrapper {
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 99;
  height: 166rpx;
  width: 100%;
  padding: 24rpx 30rpx  32rpx;
  box-sizing: border-box;
  background:rgba(255,255,255,1);
  box-shadow:0px 0px 16px 0px rgba(25,25,26,0.04), 0px 0px 24px 0px rgba(25,25,26,0.08);
}
.score-wrapper .score {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.score-wrapper .score .left{
  flex: 1;
  display: flex;
  align-items: center;
}
.score-wrapper .score .right{
  flex: 1;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.score-wrapper .score .left text , .score-wrapper .score .right text{
  color: #A1A2A6;
  font-size: 28rpx;
}
.score-wrapper .score .right .text {
  font-size:28rpx;
  font-weight:bold;
  color:rgba(25,25,26,1);
  margin: 0 24rpx 0 17rpx;
}
.score-wrapper .score .left input {
  width:124rpx;
  height:68rpx;
  color: #19191A;
  background:rgba(255,255,255,1);
  border:1rpx solid rgba(161,162,166,1);
  border-radius:6rpx;
  margin: 0 17rpx 0 23rpx;
  font-size: 30rpx;
}
.score-wrapper .info {
  width:100%;
  text-align: center;
  font-size:24rpx;
  font-weight: bold;
  color:rgba(255,96,71,1);
}
.score-wrapper .button {
  position: absolute;
  bottom: 0;
  left: 0;
  width:100%;
  height:96rpx;
  text-align: center;
  line-height: 96rpx;
  font-size: 34rpx;
  font-weight: 500;
  color: #FFF;
  background:linear-gradient(90deg,rgba(255,122,0,1) 0%,rgba(255,170,0,1) 100%);
}
.score-wrapper-item {
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 88rpx;
}
.score-wrapper-item .left{
  flex: 1;
  display: flex;
  align-items: center;
  font-size: 28rpx;
}
.score-wrapper-item .left image{
  height: 32rpx;
  width: 32rpx;
}
.score-wrapper-item .left input{
  flex: 0 0 50rpx;
}
.score-wrapper-item .left text:nth-child(1){
  color: #818D99;
  font-size: 28rpx;
  margin-right: 46rpx;
}
.score-wrapper-item .right {
  display: flex;
  align-items: center;
}
.score-wrapper-item .right text{
  color: #818D99;
  font-size: 28rpx;
}

.answer-wrapper {
  display: flex;
  justify-content: space-between;
  align-items: center;
  line-height: 88rpx;
  font-size: 28rpx;
  color: #818D99;
  background:#F9FAFC;
  border-radius:16rpx 16rpx 0 0;
  padding: 0 80rpx 0 64rpx;
  margin-top: 24rpx;
}
.warn-info {
  position: absolute;
  width: 100%;
  height: 64rpx;
  top: 0;
  left: 0;
  background:rgba(255,203,82,.32);
  display: flex;
  justify-content: center;
  align-items: center;
  color: #FFAA00;
  font-size: 24rpx;
}
.warn-info image{
  width: 24rpx;
  height: 24rpx;
  margin-right: 7rpx;
}
.feedback-success {
  width:356rpx;
  height:107rpx;
  line-height: 107rpx;
  text-align: center;
  background:rgba(255,255,255,1);
  border-radius:21rpx;
  font-size: 30rpx;
  color: #1D2023;
}
.feedback-wrapper {
  position: fixed;
  z-index: 999;
  width:539rpx;
  height:352rpx;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  display: flex;
  justify-content: center;
  align-items: center;
}
.feedback-wrapper .content{
  width:539rpx;
  height:352rpx;
  background:rgba(255,255,255,1);
  border-radius:21rpx;
  /* position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%); */
}
.feedback-wrapper .content .text{
  font-size: 30rpx;
  line-height: 48rpx;
  color: #1D2023;
  padding: 68rpx 32rpx 75rpx;
  box-sizing: border-box;
}
.feedback-wrapper .content .btn-wrapper{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.feedback-wrapper .content .btn-wrapper view {
  flex: 1;
  text-align: center;
  height: 85rpx;
  font-size:30rpx;
  line-height: 85rpx;
}
.feedback-wrapper .content .btn-wrapper view:nth-child(1){
  background:rgba(215,220,224,1);
  border-radius:0px 0px 0px 21px;
  color: #1D2023;
}
.feedback-wrapper .content .btn-wrapper view:nth-child(2){
  background:rgba(92,179,255,1);
  border-radius:0px 0px 21px 0px;
  color: #FFF;
}
.feedback-layer {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 99;
  background:rgba(29,32,35,.64);
}

.disabled {
  opacity: .4;
}