/* <!-- page.acss --> */
.overview {
  /* margin-top: 24rpx; */
}
.overview .title{
    display: flex;
    justify-content: flex-start;
    align-items: center;
    height: 88rpx;
    background:#F9FAFC;
    font-size:34rpx;
    font-weight:bold;
    padding-left: 25rpx;
    padding-right: 22rpx;
    box-sizing: border-box;
    color:rgba(29,32,35,1);
    border-radius:16rpx 16rpx 0 0;
}
.overview .title image{
    width: 30rpx;
    height: 30rpx;
    margin-right: 16rpx;
}
.chart-wrapper {
  width: 690rpx;
  padding-bottom: 32rpx;
  margin: 20rpx auto 20rpx;
  background-color: #fff;
  border-radius:16rpx;
}
.container-column {
  height: 300px;
  width: 100%;
} 
canvas {
  width: 100%;
  height: 100%;
}
.ratio-title {
  color: #19191A;
  font-size: 32rpx;
  text-align: center;
}
.ratio-tap {
  display: flex;
  align-items: center;
  justify-content: flex-end;
  padding-right: 30rpx;
  margin-top: 30rpx;
}
.ratio-tap text{
  font-size: 28rpx;
  margin-left: 15rpx;
}
.top-five .c-title{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 32rpx;
  text-align: center;
  width: 100%;
  color: #fff;
  border-radius: 30rpx;
  background: #ffc800;
}
.table {
  padding: 0 30rpx;
}
.table .thead,.table .tbody{
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #f5f5f5;
}
.table .thead view ,.table .tbody view{
  font-size: 32rpx;
  flex: 1;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.table .tbody .check{
  flex: 0 0 60rpx;
  color: #fea304;
  font-size: 25rpx;
}

.model {
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  background-color: rgba(0,0,0,0.5);
  opacity: 1;
  visibility: visible;
  transition: opacity 0.3s 0s, visibility 0s 0.3s;
}
.model-wrapper {
  height: 100%;
  width: 100%;
  display: flex;
  justify-content: center;
  flex-direction: row;
  align-items: center;
}
.model-content {
  width: 500rpx;
  height: auto;
  padding-bottom: 30rpx;
  box-sizing: border-box;
  position: relative;
  background-color: #fff;
  border-radius: 8rpx 8rpx 15rpx 15rpx;
  text-align: center;
}
.model-container {
  position: absolute;
  height: 100%;
  width: 100%;
}
.model-content .icon {
  position: absolute;
  top: 8rpx;
  right: 8rpx;
  width: 30rpx;
  height: 30rpx;
}
.model-content .icon::before, .model-content .icon::after {
  content: '';
  position: absolute;
  top: 12rpx;
  width: 14rpx;
  height: 3rpx;
  background-color: #8f9cb5;
}
.model-content .icon::before {
  transform: rotate(45deg);
  left: 8rpx;
}
.model-content .icon::after {
  transform: rotate(-45deg);
  right: 8rpx;
}
.model-content .model-title{
  font-size: 32rpx;
  text-align: center;
  font-weight: bold;
  margin: 40rpx 0 30rpx;
}
.model-content .ul{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}
.model-content .ul .li{
  flex: 0 0 50%;
  text-align: center;
  line-height: 40rpx;
  margin-top: 15rpx;
}

.score-info{
  padding: 0 19rpx;
  margin: 20rpx 25rpx 0;
  box-sizing: border-box;
  background:rgba(255,255,255,1);
  box-shadow:0rpx 8rpx 32rpx 0px rgba(29,32,35,0.08);
  border-radius:16rpx;
}
 .score-info-wrapper {
    
    height:160rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
 }
.score-info-wrapper .score-item{
    display: flex;
    flex-direction: column;
    align-items: center;
}
.score-info-wrapper .score-item view:nth-child(1){
    font-size:28rpx;
    font-weight:400;
    color:rgba(29,32,35,1);
}
.score-info-wrapper .score-item view:nth-child(2){
    font-size:40rpx;
    font-weight:500;
    color:rgba(29,32,35,1);
}
.score-info-wrapper .score-item .lose-score {
    font-size:40rpx;
    font-weight:500;
    color:#FF6047 !important;
    position: relative;
}
.score-info-wrapper .score-item .lose-score::after {
    content: '';
    bottom:-4rpx;
    left: 0;
    height: 2rpx;
    width: 100%;
    background-color: #FF6047;
    position: absolute;
}

.score-info .info{
  font-size:34rpx;
  font-weight: bold;
  color:rgba(29,32,35,1);
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.score-info .info view{
  width:20rpx;
  height:20rpx;
  background:rgba(255,170,0,1);
  border-radius:50%;
  margin-right: 17rpx;
}