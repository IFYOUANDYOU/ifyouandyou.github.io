page{
  height: 100vh;
}
.wrapper{
  width: 100%;
  height: 100%;
  background-color:#EBF0F5 ;
  overflow: hidden;
}
.nav{
  width: 690rpx;
  margin: 44rpx auto 47rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.nav .title{
  font-size: 34rpx;
  color: #1D2023;
  font-weight: bold;
  max-width: 600rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.nav .menus{
  font-size: 30rpx;
  color: #565E66;
  font-weight: bold;
  letter-spacing:3rpx;
}
.btn{
  width: 100%;
  height: 96rpx;
  text-align: center;
  line-height: 96rpx;
  font-size:34rpx;
  color: #fff; 
  position: absolute;
  left: 0;
  bottom: 0;
  background:linear-gradient(90deg,rgba(255,160,51,1) 0%,rgba(255,203,82,1) 100%);
}
.wrapper .model{
  background:linear-gradient(90deg,rgba(255,217,173,1) 0%,rgba(255,234,185,1) 100%);
}
.tablelist{
  width: 690rpx;
  margin: 0 auto;
  border-radius: 16rpx;
  overflow: hidden;
}
.thead,
.tbody{
  width: 100%;
  display: flex;
  height: 90rpx;
  border-bottom: 1rpx solid #F6F8FA;
  background-color: #fff;
}
.tablelist .scroll_view{
  /* max-height: 710rpx; */
  max-height: calc(100vh - 384rpx);
  overflow-y: scroll;
  -webkit-overflow-scrolling: touch;
}
.thead{
  background-color: #F9FAFC;
  color: #818D99;
  border: none;
}
.tbody:last-child{
  border-bottom:0; 
}
.thead .td{
  height: 100%;
  line-height: 90rpx;
  font-size: 28rpx;
  color: #818D99;
}
.thead .td:nth-child(1),
.tbody .td:nth-child(1){
  width: 183rpx;
  margin-left: 32rpx;
  /*88rpx*/
}
.tbody .td:nth-child(1){
  font-size: 28rpx;
  line-height: 90rpx;
  font-weight: 600;
}
.thead .td:nth-child(2),
.tbody .td:nth-child(2){
  width: 200rpx;
  margin-left: 20rpx;
  text-align: center;
  /*110*/
}
.td_pro{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.td_pro .score{
  font-size: 28rpx;
  color: #1D2023;
  font-weight: bold;
  margin-bottom: 8rpx;
}
.td_pro .progress{
  width: 200rpx;
  height: 8rpx;
  border-radius: 4rpx;
  background-color: #C2ECFD;
  position: relative;
}
.progress .progressBar{
  height: 100%;
  border-radius: 4rpx;
  background-color: #00AEF7;
  position: absolute;
  top: 0;
  left: 0;
  transition: width 0.3s linear;
}
.thead .td:nth-child(3),
.tbody .td:nth-child(3){
  width: 110rpx;
  margin-left: 110rpx;
}
.tbody .td:nth-child(3){
  display: flex;
  align-items: center;
}
.bt{
  width: 104rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  background:linear-gradient(90deg,rgba(255,160,51,1) 0%,rgba(255,203,82,1) 100%);
  border-radius:4px;
}
.complete{
  background: #00AEF7;
}
.no_data{
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  color: #A1A2A6;
}
.propmodel{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  background-color: rgba(0,0,0,0.5);
}
.box{
  width:277rpx;
  height: 264rpx;
  background-color: #fff;
  border-radius:8px 0px 8px 8px;
  position: absolute;
  top: 84rpx;
  right: 32rpx;
}
.box .options{
  margin: 31rpx 0 50rpx 32rpx;
  display: flex;
}
.options .icons{
  width: 32rpx;
  height: 32rpx;
  margin-right: 16rpx;
  position: relative;
  top: 1rpx;
}
.icons image{
  width: 100%;
  height: 100%;
  display: block;
}
.options .b_tit{
  font-size: 28rpx;
  color: #1D2023;
}