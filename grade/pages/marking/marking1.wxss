  height: 6.4vmin;
}

.topbar .select {
  width: 34.8vmin;
  height: 9.6vmin;
  border: 0.13333vmin solid #C4C8CC;
  border-radius: 1.06667vmin;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  position: relative;
  margin-right: 1.6vmin;
}

.topbar .select .student_name {
