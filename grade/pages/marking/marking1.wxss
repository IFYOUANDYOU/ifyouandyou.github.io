/* topbar */
.topbar {
  width: 100%;
  height: 12.8vmin;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-pack: distribute;
      justify-content: space-around;
  -webkit-box-align: center;
      -ms-flex-align: center;
          align-items: center;
  background-color: #fff;
  position: absolute;
  left: 0vmin;
  top: 0vmin;
}

.topbar .item {
  width: 9.6vmin;
  height: 12.8vmin;
  position: relative;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
      -ms-flex-pack: center;
          justify-content: center;
  -webkit-box-align: center;
      -ms-flex-align: center;
          align-items: center;
}

.topbar .handle {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0vmin;
  top: 0vmin;
  z-index: 99;
}

.topbar .item image {
