}
.swiper-active{
  animation: rights 0.2s linear;
}
.swiper-actived{
  animation: lefts 0.2s linear;
}

@keyframes lefts{
  from{transform: translateX(-100%)}
  to{transform: translateX(0)}
}
@keyframes rights{
  from{transform: translateX(100%)}
  to{transform: translateX(0)}
}


/* keybar 打分键盘 */
.keybar {
  width: 100%;
  height: auto;
  background-color: #fff;
  position: absolute;
  bottom: 0;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
      -ms-flex-pack: center;
          justify-content: center;
  -webkit-box-align: center;
      -ms-flex-align: center;
          align-items: center;
  -webkit-transition: height .3s;
  transition: height .3s;
}

.keybar .keycode {
  width: 92vmin;
  /* height: tovmin(174); */
  height: auto;
  padding: 2.66667vmin;
  -webkit-box-flex: 0;
      -ms-flex: none;
          flex: none;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-wrap: wrap;
      flex-wrap: wrap;
  position: relative;
  -webkit-box-pack: justify;
      -ms-flex-pack: justify;
          justify-content: space-between;
}

.keybar .keycode .code {
  width: 12vmin;
  height: 12vmin;
  font-size: 4.53333vmin;
  color: #1D2023;
  font-weight: 500;
  line-height: 12vmin;
  text-align: center;
  position: relative;
  margin: 0.53333vmin 0.26667vmin;
  -webkit-box-flex: 0;
      -ms-flex: 0 0 auto;
          flex: 0 0 auto;
  border: 0.13333vmin solid #D7DCE0;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border-radius: 1.06667vmin;
}

.keybar .keycode_active {
  background-color: #565E66;
  color: #fff;
  border-radius: 1.06667vmin;
}

.keybar .code image {
  width: 100%;
  height: 100%;
}

.keybar .code-text {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0vmin;
  top: 0vmin;
  line-height: 12vmin;
  text-align: center;
}

.keybar .text_active {
  color: #fff;
}

.keybar .bubble {
  width: 16vmin;
  height: 16vmin;
  position: absolute;
  left: -1.86667vmin;
  top: -18.66667vmin;
  z-index: 10;
  background-color: #565E66;
  border-radius: 1.06667vmin;
}

.keybar .bubble::after {
  content: '';
  display: inline-block;
  width: 2.93333vmin;
  height: 2.93333vmin;
  background-color: #565E66;
  -webkit-transform: rotate(45deg) translateX(-50%);
          transform: rotate(45deg) translateX(-50%);
  position: absolute;
  left: 50%;
  bottom: -2.66667vmin;
}

.keybar .bubble image {
  width: 100%;
  height: 100%;
}

.keybar .bubble text {
  font-size: 6.4vmin;
  color: #fff;
  position: absolute;
  left: 50%;
  top: 50%;
  -webkit-transform: translate(-50%, -55%);
          transform: translate(-50%, -55%);
}

.keybar .score {
  width: 92vmin;
  height: auto;
  position: absolute;
  left: 4vmin;
  top: -11.06667vmin;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
}

/* retract */
.retract {
  width: 10.8vmin;
  height: 10.66667vmin;
  position: relative;
}

.retract image {
  width: 100%;
  height: 100%;
}


/* magnifier  放大图片按钮 */
.magnifier {
  width: 14.93333vmin;
  height: 14.93333vmin;
  position: absolute;
  right: 3.73333vmin;
  bottom: 38.93333vmin;
  z-index: 100;
}

.magnifier image {
  width: 100%;
  height: 100%;
  display: block;
}

/* score_bar */
.score_bar {
  width: 8.53333vmin;
  height: 8.53333vmin;
  -webkit-box-sizing: border-box;
          box-sizing: border-box;
  border-radius: 50%;
  color: #fff;
  font-weight: 600;
  font-size: 4.8vmin;
  background-color: #33AAFF;
  text-align: center;
  line-height: 8.53333vmin;
  margin-right: 1.33333vmin;
}

.score_bar .wait_bar {
  border: 0.13333vmin solid #33AAFF;
  background-color: #CAE4F7;
}

.score_bar .dotted_bar {
  border: 0.13333vmin dashed #33AAFF;
  background-color: transparent;
}

/* 横屏下样式 */
.landscape {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
}

@media all and (orientation: landscape) {
  .topbar, ._box, .keybar {
    position: static;
    height: 100%;
  }
  /* 横屏下  topbar */
  .topbar {
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
        -ms-flex-direction: column;
            flex-direction: column;
    width: 12.8vmin;
    height: 100%;
  }
  .topbar .item {
    margin: 0;
    width: 12.8vmin;
    height: 12.8vmin;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: center;
        -ms-flex-pack: center;
            justify-content: center;
    -webkit-box-align: center;
        -ms-flex-align: center;
            align-items: center;
  }
  .topbar .item image {
    width: 6.4vmin;
    height: 6.4vmin;
  }
  /* 横屏下  _box */
  ._box {
    -webkit-box-flex: 1;
        -ms-flex: 1;
            flex: 1;
    -webkit-transform: none;
            transform: none;
  }
  /* 横屏下  keybar */
  .keybar {
    width: auto;
    height: 100%;
    right: 0;
  }
  .keybar .keycode {
    width: auto;
    height: 38.13333vmin;
    -webkit-box-orient: vertical;
    -webkit-box-direction: normal;
        -ms-flex-direction: column;
            flex-direction: column;
  }
}

